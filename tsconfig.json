{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/middleware.js", "src/app/components/location/ClientLocationOverride.jsx", "src/app/lib/store/hooks/index.js", "src/app/lib/store/features/user/userSlice.js", "src/app/lib/store/store.js", "src/app/(pages)/(auth)/signup/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
